{
    "manifest_version": 3,
    "name": "YouTube Ad Blocker Detector",
    "version": "1.0",
    "description": "Reload YouTube page if an ad blocker message is detected.",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}
